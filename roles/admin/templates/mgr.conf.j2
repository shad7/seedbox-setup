[program:mgr]
command = {{ ansible_env.HOME }}/webapps/{{ ansible_hostname }}/bin/gunicorn_start                    ; Command to start app
user = {{ ansible_ssh_user }}                                                         ; User to run as
stdout_logfile = {{ ansible_env.HOME }}/webapps/{{ ansible_hostname }}/logs/gunicorn_supervisor.log   ; Where to write log messages
redirect_stderr = true                                                ; Save stderr in the same log
