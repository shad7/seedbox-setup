alias ls='ls --color=auto'
alias dir='dir --color=auto'
alias vdir='vdir --color=auto'
alias grep='grep --color=auto'
alias fgrep='fgrep --color=auto'
alias egrep='egrep --color=auto'
alias ll='ls -l'
alias la='ls -A'
alias l='ls -CF'
alias vi='vim'
alias cd..='cd ..'

alias space='du ~/ -s --si'
alias ldel='pgrep -d " " -u shad7 -f deluge'
alias kdel='pkill -u shad7 -f deluge'
alias myps='ps -fH -u shad7'
alias stsup='kill -TERM $(cat $HOME/.local/etc/supervisord.pid)'
alias ksup='kill -KILL $(cat $HOME/.local/etc/supervisord.pid)'
alias stngx='kill -TERM $(cat $HOME/.nginx/pid $HOME/.nginx/php/pid)'
alias kngx='kill -KILL $(cat $HOME/.nginx/pid $HOME/.nginx/php/pid)'
